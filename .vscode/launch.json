  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'web_server_rs'",
      "cargo": {
        "args": [
          "build",
          // "--lib",
          "--bin=web_server_rs",
          "--package=web_server_rs"
        ],
        "filter": {
          "name": "web_server_rs",
          "kind": "bin"
        }
      },
      "args": ["--url", "https://localhost/"],
      "cwd": "${workspaceFolder}",
      // "cwd": "${workspaceRoot}/target/debug/",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"]
      // "program": "${cargo:program}",
    }
  ]
}
